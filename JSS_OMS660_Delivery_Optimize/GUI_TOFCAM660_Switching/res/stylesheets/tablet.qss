QLabel#lAxisOffsetZ,
QLabel#lAxisLineWidth,
QLabel#lAxisTransparent,
QLabel#lCellWidth,
QLabel#lNumberCells,
QLabel#lGridTransparent,
QLabel#lGridOffsetX,
QLabel#lGridOffsetY,
QLabel#lGridOffsetZ,
QLabel#lLensAngle,
QLabel#lAngleZ,
QLabel#lObjectOffsetX,
QLabel#lObjectOffsetY,
