*{
    font-family: arial;
    font-size: 14pt;
}

QWidget{
    background-color:rgb(237, 239, 238);
}

QWidget#wStatus{
    background-color:rgb(237, 239, 238);
}

QWidget#WidgetPointCloudSettings{

    min-width: 1800px;
    width: 1800px;
}


QGroupBox{
    border:none;
}

/********* QCheckBox ********************************/

QCheckBox{
    background-color:transparent;
}

QCheckBox#cbGrayPointCloud::indicator,
QCheckBox#cbSurfaceInterconnect::indicator,
QCheckBox#cbAxis3D::indicator,
QCheckBox#cbAxis2D::indicator,
QCheckBox#cbVerticalFlip::indicator,
QCheckBox#cbHorizontalFlip::indicator,
QCheckBox#cbGrid::indicator,
QCheckBox#cbPoints::indicator,
QCheckBox#cbCartesian::indicator,
QCheckBox#cbUndistortion::indicator,
QCheckBox#cbAutoRotating::indicator,
QCheckBox#checkBoxAutoScale::indicator,
QCheckBox#checkBoxShowLeft::indicator,
QCheckBox#checkBoxShowRight::indicator
{
    width: 40px;
    height: 40px;
}

QCheckBox::indicator{
    width:60px;
    height:60px;
    background-color:rgb(35, 142, 194);
    border: 2px solid rgb(0, 0, 0);
}

QCheckBox::indicator:checked {
    image: url(:/icons/images/icons/checkbox_checked_80_80.png);
}


QCheckBox::indicator:focus{
    border: 0px solid rgb(0, 0, 0);
}

QRadioButton::indicator{
    width:60px;
    height:60px;
    background-color:rgb(35, 142, 194);
    border-radius: 30px;
}

/*************************************************/

QRadioButton::indicator:checked{
    image: url(:/icons/images/icons/radiobutton_checked.png);
}

QRadioButton::indicator:focus{
    border: 0px solid rgb(128, 128, 128);
}

/*************************************************/

QComboBox {
    background-color:rgb(255,255,255);
    border: 2px solid rgb(0, 0, 0);
    padding-left: 10px;
    font: 14pt;
    min-width: 240px;
    min-height: 80px;
}

QComboBox::drop-down {
    background-color:rgb(255,255,255);
 }

QComboBox:focus{
    border: 1px solid rgb(255, 255, 255);
}

/**************************************************/

QSpinBox, QDoubleSpinBox {
    background-color:rgb(255,255,255);
    border: 1px solid rgb(0, 0, 0);
    padding-left: 10px;
    width: 320px;
    min-width: 320px;
    height: 80px;
}

QSpinBox::up-button, QDoubleSpinBox::up-button {
    image: url(:/icons/images/icons/spinbox_right.png);
    border: 2px solid rgb(0, 0, 0);
}

QSpinBox::down-button, QDoubleSpinBox::down-button {
    image: url(:/icons/images/icons/spinbox_left.png);
    border: 2px solid rgb(0, 0, 0);
}

QSpinBox::up-button:hover, QDoubleSpinBox::up-button:hover{    
    border: 2px solid rgb(0, 0, 0);
    image: url(:/icons/images/icons/spinbox_right_hover.png);
}

QSpinBox:down-button:hover, QDoubleSpinBox:down-button:hover{    
    border: 2px solid rgb(0, 0, 0);
    image: url(:/icons/images/icons/spinbox_left_hover.png);
}


QSpinBox::up-button {
    subcontrol-origin: border;
    subcontrol-position: right;
    width: 80px;
    height: 80px;
}

QSpinBox::down-button {
    subcontrol-origin: border;
    subcontrol-position: left;
    width: 80px;
    height: 80px;
}

QDoubleSpinBox::up-button {
    subcontrol-origin: border;
    subcontrol-position: right;
    width: 80px;
    height: 80px;
}

QDoubleSpinBox::down-button {
    subcontrol-origin: border;
    subcontrol-position: left;
    width: 80px;
    height: 80px;
}


QSpinBox#sbSurfaceInterconnect::up-button,
QSpinBox#sbSurfaceInterconnect::down-button,
QDoubleSpinBox#dbPointSize::up-button,
QDoubleSpinBox#dbPointSize::down-button
{
    width: 60px;
    height: 60px;
}

QSpinBox#sbSurfaceInterconnect{
    min-width: 160px;
    width: 160px;
    min-height: 60px;
    height: 60px;
}

QDoubleSpinBox#dbPointSize{
    min-height: 60px;
    height: 60px;
    min-width: 160px;
    width: 160px;
}


QDoubleSpinBox#sbFps{
    min-width: 210px;
    width: 210px;

}


QSpinBox#spinBoxLeftMin::up-button,
QSpinBox#spinBoxLeftMax::up-button,
QSpinBox#spinBoxRightMin::up-button,
QSpinBox#spinBoxRightMax::up-button,
QSpinBox#spinBoxNumberOfValues::up-button,

QSpinBox#spinBoxLeftMin::down-button,
QSpinBox#spinBoxLeftMax::down-button,
QSpinBox#spinBoxRightMin::down-button,
QSpinBox#spinBoxRightMax::down-button,
QSpinBox#spinBoxNumberOfValues::down-button
{
    width: 60px;
    height: 60px;
}



QSpinBox#spinBoxNumberOfValues,
QSpinBox#spinBoxLeftMin,
QSpinBox#spinBoxLeftMax,
QSpinBox#spinBoxRightMin,
QSpinBox#spinBoxRightMax,
QSpinBox#spinBoxNumberOfValues
{
    min-height: 60px;
    height: 60px;
    min-width: 160px;
    width: 160px;
}

QSpinBox#sbTopLeftX,
QSpinBox#sbTopLeftY,
QSpinBox#sbBottomRightX,
QSpinBox#sbBottomRightY
{
    min-height: 80px;
    height: 80px;
    min-width: 240px;
    width: 240px;

}


QDoubleSpinBox#dbAxisTransparent,
QDoubleSpinBox#sbdTransparent,
QDoubleSpinBox#dbScaleX, QDoubleSpinBox#dbScaleY, QDoubleSpinBox#dbScaleZ,
QDoubleSpinBox#dbZoom,
QSpinBox#sbAxisOffsetX, QSpinBox#sbAxisOffsetY, QSpinBox#sbAxisOffsetZ,
QSpinBox#sbAxisLineWidth,
QSpinBox#sbCellWidth,
QSpinBox#sbNumCells,
QSpinBox#sbOffsetX, QSpinBox#sbOffsetY, QSpinBox#sbOffsetZ,
QSpinBox#sbLensAngle,
QSpinBox#sbAngleObjectZ,
QSpinBox#sbObjectOffsetX, QSpinBox#sbObjectOffsetY, QSpinBox#sbObjectOffsetZ,
QSpinBox#sbAngleX, QSpinBox#sbAngleY, QSpinBox#sbAngleZ,
QSpinBox#sbCameraOffesetX, QSpinBox#sbCameraOffesetY
{
    min-width: 280px;
    width: 280px;
    font-size: 13pt;
}



/*********** QToolButton, QPushButton ************************/

QToolButton, QPushButton{
    padding:5px;
    background-color:rgb(35, 142, 194);
    border: 2px solid rgb(0, 0, 0);
    color:rgb(255,255,255);
    min-width: 200px;
    min-height: 80px;
}

QToolButton:hover, QPushButton:hover{
    background-color:rgb(255, 214, 106);
    color:rgb(0, 0, 0);
}

QToolButton:!enabled, QPushButton:!enabled{
    background-color:transparent;
    color:rgb(0, 0, 0);
}

QToolButton:focus, QPushButton:focus{
    border: 1px solid rgb(255, 255, 255);
}

QPushButton#pushButtonLogo{
    width: 320x;
    height: 160px;
    min-width: 320px;
    min-height: 160px;
}

QToolButton#tbFront, QToolButton#tbSide, QToolButton#tbTop{
    height: 60px;
    min-height: 60px;

    width: 80px;
    min-width: 80px;
}

QToolButton#tbPixelInfo,
QToolButton#tbPixelScope,
QToolButton#tbTimeScope,
QToolButton#tbDefaultRoi{

    height: 60px;
    min-height: 60px;

}

QToolButton#tbRoi,
QToolButton#tbShowControls, QToolButton#tbReset, QToolButton#tbFullScreenPC, QToolButton#tbSettings{
    height: 60px;
    min-height: 60px;

    width: 150px;
    min-width: 150px;
}

QToolButton#tbFullScreen{
    height: 60px;
    min-height: 60px;
}

QPushButton#pushButtonSingle, QPushButton#pushButtonStream{

    qproperty-iconSize: 64px;
}


QToolButton#tbRecord, QToolButton#tbPlay, QToolButton#tbStop,
QPushButton#pushButtonSingle, QPushButton#pushButtonStream
{
    min-width: 200px;
    width: 200px;
    min-height: 80px;
    height: 80px;
}

